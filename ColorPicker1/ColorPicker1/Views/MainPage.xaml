<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="ColorPicker1.Views.MainPage"
             Title="{Binding Title}">
    <StackLayout x:Name="OuterStackLayout"
                 HorizontalOptions="Center"
                 VerticalOptions="Center">
        <Button Text="BoxView Picker"
                Command="{Binding NavToBoxViewPickerCommand}" />
        <Button Text="Static Picker"
                Command="{Binding NavToStaticPickerCommand}" />
    </StackLayout>
</ContentPage>
    